<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <fields>
        <fullName>Contact_Source__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <label>Contact Source</label>
        <length>80</length>
        <required>false</required>
        <trackFeedHistory>false</trackFeedHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Follow_Status__c</fullName>
        <deprecated>false</deprecated>
        <description>Click on link to change following status</description>
        <externalId>false</externalId>
        <formula>IF(followed__c == true,
HYPERLINK(&apos;/apex/sf4twitter__TwitterFollow?cid=&apos;+Id, &apos;Yes&apos;),
HYPERLINK(&apos;/apex/sf4twitter__TwitterFollow?cid=&apos;+Id, &apos;No&apos;))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Click on link to change following status</inlineHelpText>
        <label>Following On Twitter</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Influencer_Type__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <label>Influencer Type</label>
        <picklist>
            <controllingField>Influencer__c</controllingField>
            <picklistValues>
                <fullName>Reporter</fullName>
                <controllingFieldValues>Yes</controllingFieldValues>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Other</fullName>
                <controllingFieldValues>Yes</controllingFieldValues>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>None</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <trackFeedHistory>false</trackFeedHistory>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Influencer__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>Is this Contact an Influencer (ie Reporter?)</inlineHelpText>
        <label>Influencer</label>
        <picklist>
            <picklistValues>
                <fullName>No</fullName>
